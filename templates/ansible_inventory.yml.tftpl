---
# Ansible inventory
all:
  hosts:
%{ for node in nodes ~}
    ${node}:
      ansible_host: ${node_ips_map[node]}
      ansible_user: test
      ansible_ssh_private_key_file: ${ssh_key_filename}
      ansible_ssh_common_args: >-
        -o Compression=no
        -o UserKnownHostsFile=/dev/null
        -o StrictHostKeyChecking=no
%{ endfor ~}
